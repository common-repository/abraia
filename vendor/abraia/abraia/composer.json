{
  "name": "abraia/abraia",
  "version": "0.5.1",
  "description": "Abraia PHP SDK",
  "keywords": [
    "image-optimization",
    "image-compression",
    "image-processing",
    "abraia-api",
    "php"
  ],
  "homepage": "https://github.com/abraia/abraia-php#readme",
  "license": "MIT",
  "authors": [{
    "name": "Jorge Rodriguez Araujo",
    "email": "jorge@abraiasoftware.com"
  }],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "php-coveralls/php-coveralls": "^2.1",
    "vlucas/phpdotenv": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Abraia\\": "abraia/"
    }
  },
  "scripts": {
    "test": "phpunit tests/test.php"
  },
  "archive": {
    "exclude": [".gitignore", ".travis.yml", "/images", "/tests", "phpunit.xml"]
  }
}
